  "author": "Komi Akoumany <komi.akoumany@gmail.com>",
  "contributors": [
    "Michael Hilario <mike.hilario@gmail.com>"
  ],
  "dependencies": {
    "@ai-sdk/react": "^1.2.1",
    "@babel/polyfill": "^7.12.1",
    "@brightcove/react-player-loader": "^1.4.0",
    "@sentry/gatsby": "^6.19.7",
    "@sentry/integrations": "^6.19.7",
    "@sentry/tracing": "^6.19.7",
    "@tippyjs/react": "^4.2.6",
    "animejs": "^3.1.0",
    "aws-amplify": "^3.3.21",
