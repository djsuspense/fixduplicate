  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/eslint-parser": "^7.26.8",
    "@eslint/js": "^9.21.0",
    "@types/marked": "^6.0.0",
    "@types/node": "^14.14.37",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.16",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "prettier": "^1.18.2",
    "react-icons": "^5.5.0",
    "typed-css-modules": "^0.9.1"
  },
  "keywords": [
    "gatsby",
    "scoop news group",
    "sng"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "typed-css": "tcm src --include '**/*.module.css'",
    "disable-telemetry": "gatsby telemetry --disable",
    "build": "gatsby build --prefix-paths",
    "develop": "gatsby develop --port 8787 --open",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "yarn develop",
    "restart": "rm -rf node_modules yarn.lock .cache && nvm use 14.20.0 && yarn && yarn start",
    "clean": "gatsby clean",
    "serve": "gatsby serve",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing \"",
    "container": "gatsby build && yarn docker-run && yarn open",
    "docker-run": "docker run --name gatsby-event-starter -dit -p 8080:80 -v \"$PWD\"/public:/usr/local/apache2/htdocs/ httpd:alpine",
    "docker-log": "docker logs gatsby-event-starter",
    "container-remove": "docker rm --force gatsby-event-starter",
    "open": "open http://0.0.0.0:8080",
    "info": "gatsby info",
    "open-s3": "open http://event-dc-cyberweek.s3-website.us-east-1.amazonaws.com/",
    "deploy": "aws s3 cp ./public s3://event-dc-cyberweek --recursive --include \"*\"",
    "postdeploy": "yarn invalidate",
    "no-tracking": "gatsby telemetry --disable",
    "invalidate": "aws cloudfront create-invalidation --distribution-id E3QNVL9YL9M6PU --paths '/*'",
    "aws-upload": "aws s3 cp ./public s3://event-dc-cyberweek --recursive --include '*'",
    "aws-remove": "aws s3 rm s3://event-dc-cyberweek --recursive --exclude '2021/*' --exclude '2022/*' --exclude '2023/*' --exclude 'ci/*'",
    "predeploy": "yarn no-tracking && yarn clean && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scoopnewsgroup/gatsby-event-starter"
  },
  "bugs": {
    "url": "https://github.com/scoopnewsgroup/gatsby-event-starter/issues"
  },
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "requireConfigFile": false,
    "babelOptions": {
      "presets": [
        "@babel/preset-react",
        "@babel/preset-env"
      ]
    }
  }
}
