    "react-hook-form": "7.51.5",
    "react-markdown": "8.0.3",
    "react-modal": "^3.11.1",
    "react-redux": "^7.2.3",
    "react-select": "^5.2.1",
    "redux": "^4.0.5",
    "sass": "^1.63.3",
    "styled-components": "^5.2.0",
    "sweetalert2": "^11.3.0",
    "sweetalert2-react-content": "^4.2.0",
