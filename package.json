    "gatsby-transformer-csv": "^3.0.0",
    "gatsby-transformer-json": "^3.4.0",
    "gatsby-transformer-sharp": "^2.2.23",
    "lottie-react": "^2.4.1",
    "marked": "4.3.0",
    "prop-types": "^15.7.2",
